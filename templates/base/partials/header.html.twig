<header>
        <a class="brand" href="/" content="DND Puzzles">DND Puzzles</a>


        <nav class="main">
            {% for navItem in nav %}
                <a class="nav-item {% if navItem.active %} active{% endif %}" href="{{ path(navItem.route) }}">{{ navItem.label }}</a>
            {% endfor %}
        </nav>

    <form class="search-form">
        <input name="search" placeholder="Search" id="search">
        <button class="btn btn-secondary" type="submit">Search</button>
    </form>

</header>
